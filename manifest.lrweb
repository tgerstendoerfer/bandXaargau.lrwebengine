--[[
	Lightroom Web Gallery manifest.
	Maps template files to the actual files to be produced.
]]

importTags( "lr", "com.adobe.lightroom.default" )

AddResources {
	source='resources',
	destination='resources',
}

AddGridPages {
	template='grid.html',
	rows=model.nonCSS.numRows,
	columns=model.nonCSS.numCols,
}
